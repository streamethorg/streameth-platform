{
    "watch": [
        "src/workers",
        ".env"
    ],
    "ext": "js,ts,json",
    "ignore": [
        "src/logs/*",
        "src/swagger/*",
        "src/routes/*",
        "src/**/*.{spec,test}.ts"
    ],
    "exec": "node -r esbuild-runner/register ./src/workers/stage-transcriptions.ts --env=development",
    "env": {
        "NODE_ENV": "development"
    }
}