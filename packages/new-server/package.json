{
  "name": "streameth-new-server",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "start": "yarn run build && node -r module-alias/register ./dist/server.js",
    "dev": "nodemon --config nodemon.json",
    "build": "rm -rf ./dist && tsc --build tsconfig.prod.json",
    "build:tsc": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "format": "prettier --write \"src/**/*.ts\"",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "postinstall": "npx husky install"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@controllers": "dist/controllers",
    "@databases": "dist/databases",
    "@dtos": "dist/dtos",
    "@exceptions": "dist/exceptions",
    "@interfaces": "dist/interfaces",
    "@middlewares": "dist/middlewares",
    "@models": "dist/models",
    "@routes": "dist/routes",
    "@services": "dist/services",
    "@utils": "dist/utils"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongoose": "7.6.2",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.101",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.9",
    "esbuild-runner": "^2.2.2",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.3",
    "node-config": "^0.0.2",
    "node-gyp": "^9.1.0",
    "nodemon": "^3.0.2",
    "pm2": "^5.2.0",
    "prettier": "^3.1.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
